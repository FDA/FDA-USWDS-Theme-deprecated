{
  "name": "fda-uswds-theme",
  "version": "1.0.0",
  "description": "FDA USWDS theme",
  "main": "dist/js/uswds.min.js",
  "style": "dist/css/styles.min.css",
  "sass": "src/scss/styles.scss",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "gulp --env production",
    "prebuild:css": "npm run lint",
    "build:css": "gulp --env production sass",
    "lint": "npx stylelint --syntax=scss src/scss/**/*.scss",
    "test": "snyk test && npm run lint",
    "watch": "gulp --env development watch",
    "prettier:scss": "npx prettier --write ./src/scss/**/*.scss",
    "prestart": "gulp --env development",
    "start": "NODE_OPTIONS='--experimental-abortcontroller' gulp start",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "U.S. FDA",
  "contributors": [
    "Michael Jovel <michael.jovel@fda.hhs.gov>",
    "Ken Rogers <kenneth.rogers@fda.hhs.gov>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FDA/FDA-USWDS-Theme/"
  },
  "license": "CC0 1.0 Universal public domain dedication",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/html": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "autoprefixer": "^10.3.4",
    "babel-loader": "^8.3.0",
    "connect-livereload": "^0.6.1",
    "del": "^6.0.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "express": "^4.17.2",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-livereload": "^4.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
